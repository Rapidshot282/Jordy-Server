#낚시 직업 기능추가
on fishing:
    if {직업.%player%} is "낚시꾼":
        if fishing state is CAUGHT_FISH:
            cancel event
            if number of {미끼} in the player's inventory < 1:
                chance of 80%:  #미끼가 없을 경우 낚시 실패 확률
                    send title "&c실패" with subtitle "&f미끼가 없어서 물고기가 도망갔어요!" to player for 1.5 seconds
                    exit
            remove 1 of {미끼} from player
            set {%player%.fishing} to 1
            set {%player%.difficulty} to random integer between 4 and 7
            set {%player%.seconds} to 3.5
            set {_ln} to 1
            loop 7 times:
                if {_ln} <= {%player%.difficulty}:
                    chance of 50%:
                        set {%player%.fishing.way%{_ln}%} to "L"
                    else:
                        set {%player%.fishing.way%{_ln}%} to "R"
                if {_ln} > {%player%.difficulty}:
                    set {%player%.fishing.way%{_ln}%} to "&f"
                add 1 to {_ln}
            set {%player%.fishing.level} to 1
