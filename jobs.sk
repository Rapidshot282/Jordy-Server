options:
    j : &6[&e시스템&6]&f

variables:
    {뉴비.%player%} = true
    {기본템.%player%} = false
    {자동심기.%player%} = false
    

on first join:
    send title "&d죠르딩 서버에 처음 접속하셨군요!" with subtitle "&f자유, 경제, RPG made by 죠르딩" to player for 5 seconds
    set join message to "%player% 님이 서버에 처음 접속 했습니다! 환영해주세요!"
    set {뉴비.%player%} to true
    set {직업.%player%} to "뉴비"

on join:
    send title "&d죠르딩 서버" with subtitle "자유, 경제, RPG made by 죠르딩" to player for 3 seconds
    set join message to "Welcome back &e%player%!"
    set {미끼} to ink sac named "&b미끼"
    if {직업.%player%} is not set:
        set {직업.%player%} to "뉴비"

on quit:
    set quit message to "See you later, &e%player%!"

every 15 tick:
    loop all players:
        set {_l} to location of loop-player
        set {_msg::x} to "x:%round x-coord of {_l}%"
        set {_msg::y} to "y:%round y-coord of {_l}%"
        set {_msg::z} to "z:%round z-coord of {_l}%"


        if {scoreboard.%loop-player%} is true:
            wipe loop-player's sidebar 
            set name of sidebar of loop-player to "&9<Bold>%loop-player%"
            set score "&5<Bold>직업 : %{직업.%loop-player%}% / %{레벨.%loop-player%}%" in sidebar of loop-player to 4
            set score "&e<Bold>소지금 : %loop-player's money%G" in sidebar of loop-player to 3
            set score "시간 : %now%" in sidebar of loop-player to 2   
            set score "좌표 : %{_msg::*}%" in sidebar of loop-player to 1
            set score "바이옴 : %loop-player's biome%" in sidebar of loop-player to 0
        

command /scoreboard [<string>]:
    trigger:
        if arg 1 is "on":
            set {scoreboard.%player%} to true
            message "{@j} 프로필 활성화 됨!"

        if arg 1 is "off":
            set {scoreboard.%player%} to false
            message "{@j} 프로필 비활성화 됨! 활성화 하려면 &6/scoreboard on&f을 하세요"
            wipe player's sidebar

command /startitem:
	trigger:
		if {뉴비.%player%} is true:
			give 1 iron pickaxe named "철 곡괭이" with lore "시작의 곡괭이" to player
			give 1 iron sword named "철 칼" with lore "시작의 칼" to player
			give 1 iron axe named "철 도끼" with lore "시작의 도끼" to player
		else:
			message "{@j}<Bold>당신은 이미 직업이 있지 않습니까?! 고인물이 그러시다니 정말 실망입니다!"

command /전직관소환:
	trigger:
		if player is op:
			execute op command "/npc create &6전직관"
			wait 20 tick
			execute op command "/npc skin %player%"
			message "{@j} 전직관이 생성되었습니다."

on rightclick on entity:
    if entity's name is "&6전직관":
        if lore of player's tool contains "&6전직서":
            if name of player's tool contains "&e전직서":
                open chest with 3 rows named "&e직업선택" to player
                set slot 10 of current inventory of player to wheat named "&f농부" with lore "&f농부로 전직합니다."
                set slot 11 of current inventory of player to diamond pickaxe named "&f광부" with lore "&f광부로 전직합니다."
                set slot 12 of current inventory of player to fishing rod named "&f낚시꾼" with lore "&f낚시꾼으로 전직합니다."
        else:
            message "{@j} 전직서를 들고 전직관에게 말을 걸어주세요!" to player

on inventory click:
    if iname contains "&e직업선택":
        if click type is left mouse button with shift:
            cancel event
        else:
            if clicked raw slot is 10:
                if {직업.%player%} is "뉴비":
                    close player's inventory
                    wait 1 tick
                    remove 1 book named "&e전직서" with lore "&6전직서" from the player
                    wait 1 tick
                    set {직업.%player%} to "농부"
                    set {자동심기.%player%} to true
                    set {경험치.%player%} to 0
                    set {레벨.%player%} to 1
                    set {뉴비.%player%} to false
                    send title "&b축하합니다!&f &a전직&f에 성공하셨습니다." with subtitle "직업 : &e%{직업.%player%}%" to player for 2 seconds
            else if clicked raw slot is 11:
                if {직업.%player%} is "뉴비":
                    close player's inventory
                    wait 1 tick
                    remove 1 book named "&e전직서" with lore "&6전직서" from the player
                    wait 1 tick
                    set {직업.%player%} to "광부"
                    set {경험치.%player%} to 0
                    set {레벨.%player%} to 1
                    set {뉴비.%player%} to false
                    send title "&b축하합니다!&f &a전직&f에 성공하셨습니다." with subtitle "직업 : &e%{직업.%player%}%" to player for 2 seconds
            else if clicked raw slot is 12:
                if {직업.%player%} is "뉴비":
                    close player's inventory
                    wait 1 tick
                    remove 1 book named "&e전직서" with lore "&6전직서" from the player
                    wait 1 tick
                    set {직업.%player%} to "낚시꾼"
                    set {경험치.%player%} to "MAX"
                    set {레벨.%player%} to 5
                    set {뉴비.%player%} to false
                    send title "&b축하합니다!&f &a전직&f에 성공하셨습니다." with subtitle "직업 : &e%{직업.%player%}%" to player for 2 seconds             

command /직업경험치셋팅:
    trigger:
        if player is op:
            set {최대경험치::0} to 0
            set {최대경험치::1} to 6000
            set {최대경험치::2} to 9000
            set {최대경험치::3} to 15000
            set {최대경험치::4} to 20000
            set {최대경험치::5} to "MAX"
            set {직업최대레벨} to 5
            message "{@j}로드성공"

every 10 tick:
    loop all players:
        level_reload(loop-player)

function level_reload(p: player):
    if {경험치.%{_p}%} >= {최대경험치::%{레벨.%{_p}%}%}:
        if {레벨.%{_p}%} < {직업최대레벨}:
            add 1 to {레벨.%{_p}%}
            set {경험치.%{_p}%} to 0
            message "&6-----------------------------------------" to {_p}
            message "{@j} &a%{직업.%{_p}%}%의 &d레벨&f이 올랐습니다! (현재레벨 : %{레벨.%{_p}%}%)" to {_p}
            message "&6-----------------------------------------" to {_p}
        if {레벨.%{_p}%} = {직업최대레벨}:
            set {경험치.%{_p}%} to "MAX"
            if {만렙.%{_p}%} is not set:
                send title "&b축하합니다!&f &a%{직업.%{_p}%}%&f의 레벨이 &e최대치&f에 도달하였습니다!" to {_p} for 2 seconds
                set {만렙.%{_p}%} to 1

command /전직서:
    trigger:
        if player is op:
            give player 1 book named "&e전직서" with lore "&6전직서"
            message "{@j} 전직서가 지급되었습니다"

command /경험치주기 [<integer>] [<offlineplayer>]:
    trigger:
        if player is op:
            if arg 1 is not set:
                message "&6-----------------------------------------" to arg 2
                message "{@j} &e경험치주기사용법 &f- &e/경험치주기 &a[지급할경험치] &b[유저닉네임]" to arg 2
                message "&6-----------------------------------------" to arg 2
            else:
                if arg 2 is not set:
                    message "{@j}지급할 &b플레이어 닉네임&f을 입력해주세요!" to arg 2
                else:
                    add arg 1 to {경험치.%arg 2%}
                    message "{@j} &b%arg 2%&f님에게 &a경험치&f를 &a%arg 1%&f만큼 보냈습니다!" to arg 2
                    message "{@j} &e관리자&f로부터 &a경험치&f를 &a%arg 1%&f만큼 지급받으셨습니다!" to arg 2

command /직업:
    trigger:
        if {직업.%player%} is "뉴비":
            send title "당신은 현재 직업을 가지고 있지 않습니다! 전직을 먼저 하세요!" to player for 2 seconds
        else if {직업.%player%} is "농부":
            message "&6--------------------------------------------" to player
            message "{@j} [&4직업&e]&f:농부 &e[&d레벨&e]&f:%{레벨.%player%}% &e[&a경험치&e]&f:%{경험치.%player%}%/%{최대경험치::%{레벨.%player%}%}%" to player
            message "&6--------------------------------------------" to player
        else if {직업.%player%} is "광부":
            message "&6--------------------------------------------" to player
            message "{@j} [&4직업&e]&f:광부 &e[&d레벨&e]&f:%{레벨.%player%}% &e[&a경험치&e]&f:%{경험치.%player%}%/%{최대경험치::%{레벨.%player%}%}%" to player
            message "&6--------------------------------------------" to player
        else if {직업.%player%} is "낚시꾼":
            message "&6--------------------------------------------" to player
            message "{@j} [&4직업&e]&f:낚시꾼 &e[&d레벨&e]&f:낚시꾼은 최고레벨 고정입니다. &e[&a경험치&e]&f:%{경험치.%player%}%/%{최대경험치::%{레벨.%player%}%}%" to player
            message "&6--------------------------------------------" to player

on rightclick with paper:
    if lore of player's tool contains "사용 시 &a직업&f이 초기화가 됩니다.":
        if name of player's tool contains "&e직업초기화권":
            set {직업.%player%} to "뉴비"
            set {뉴비.%player%} to true
            delete {만렙.%player%}
            delete {경험치.%player%}
            delete {레벨.%player%}
            remove 1 paper named "&e직업초기화권" with lore "사용 시 &a직업&f이 초기화가 됩니다." from player
            send title "&a직업&f을 &b초기화&f 했습니다!" to player for 2 seconds

command /초기화권:
    trigger:
        if player is op:
            give player 1 paper named "&e직업초기화권" with lore "사용 시 &a직업&f이 초기화가 됩니다."
            message "{@j} 초기화권이 지급되었습니다." to player

# 직업 특수효과(농부)
on break of ripe wheat plant:
    if {직업.%player%} is "농부":
        if {레벨.%player%} = {직업최대레벨}:
            message "{@j} 직업 최대레벨에 도달하여 경험치를 얻지 못하였습니다."
            if player doesn't have enough space for 1 of wheat plant:
                message "{@j} &c<Bold>주의!&f<Bold> 인벤토리가 가득 차서 &b<Bold>추가효과&f<Bold>가 발동되지 않았습니다. 인벤토리를 비워주세요!"
                if {자동심기.%player%} is true:
                    if player has wheat seeds:
                        wait 8 tick
                        remove 1 wheat seeds from player
                        set block to wheat plant
                    else:
                        message "{@j} 자동심기를 사용하기에 &a씨앗&f이 부족합니다. &a인벤토리&f를 확인해주세요." to player
            else:
                cancel drops
                wheat_farm(player)
                message "{@j} &a밀&f을 &6직업효과&f로 추가 재배했습니다. &6(%{레벨.%player%}%)" to player
                if {자동심기.%player%} is true:
                    if player has wheat seeds:
                        wait 8 tick
                        remove 1 wheat seeds from player
                        set block to wheat plant
                    else:
                        message "{@j} 자동심기를 사용하기에 &a씨앗&f이 부족합니다. &a인벤토리&f를 확인해주세요." to player
        if {레벨.%player%} < {직업최대레벨}:
            message "{@j} &a경험치&f가 3 상승했습니다." to player
            add 3 to {경험치.%player%}
            if player doesn't have enough space for 1 of wheat plant:
                message "{@j} &c<Bold>주의!&f<Bold> 인벤토리가 가득 차서 &b<Bold>추가효과&f<Bold>가 발동되지 않았습니다. 인벤토리를 비워주세요!"
                if {자동심기.%player%} is true:
